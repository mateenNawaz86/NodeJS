  <body>
    <%- include('../includes/nav'); %>
    <main>
      <% if(products.length > 0 ) { %>
          <ul>
            <% products.forEach(item => { %>
              <li><%= item.productData.title %> (<%= item.qty %>)</li>
              <% }) %>
          </ul>
        <% } else { %>
          <h1>NO Product Found in CART!!</h1>
          <% } %>
    </main>
    <%- include('../includes/end'); %>